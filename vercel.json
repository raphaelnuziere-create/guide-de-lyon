{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/annuaire.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/evenements.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/tarifs.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/entreprise.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/debug-directus.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    {
      "source": "/css/:path*",
      "destination": "/public/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/public/js/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/public/images/:path*"
    },
    {
      "source": "/annuaire",
      "destination": "/api/annuaire.php"
    },
    {
      "source": "/entreprise/:slug",
      "destination": "/api/entreprise.php?slug=:slug"
    },
    {
      "source": "/evenements",
      "destination": "/api/evenements.php"
    },
    {
      "source": "/tarifs",
      "destination": "/api/tarifs.php"
    },
    {
      "source": "/debug-directus",
      "destination": "/api/debug-directus.php"
    },
    {
      "source": "/((?!api|public|_next).*)",
      "destination": "/api/index.php"
    }
  ]
}